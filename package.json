{
  "name": "app-scanner",
  "version": "1.0.0",
  "description": "App repository as an example scanner app",
  "main": "src/index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "oak": "~6.0.1",
    "standard": "~14.3.1"
  },
  "scripts": {
    "start": "node_modules/.bin/oak .",
    "dev": "env $(cat .env | grep -v '#' | xargs) npm start",
